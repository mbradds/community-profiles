  "sideEffects": false,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "description": "",
  "main": "./src/entry_points/trans_mountain.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm server.ts",
