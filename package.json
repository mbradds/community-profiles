    "get-tm-data": "conda run -n community-profiles python src/data_management/get_cer_files.py",
    "gis": "conda run -n community-profiles python src/data_management/cer_gis.py",
    "community-update": "npm run images && conda run -n community-profiles python src/data_management/process_communities.py",
    "prettier-check": "npx prettier --check .",
    "prettier": "npx prettier --write .",
    "lint": "npx eslint . --ext .ts"
