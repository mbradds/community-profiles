{
  "name": "community-profiles",
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "images": "node scripts/convert_images.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "set NODE_OPTIONS=--max_old_space_size=15192 && webpack --config webpack.prod.js",
    "analyze-build": "webpack --config webpack.analyze.js",
    "dev": "set NODE_OPTIONS=--max_old_space_size=15192 && webpack serve --config webpack.dev.js",
    "minify-maps": "sh ./scripts/maps_min.sh",
    "get-map-data": "conda run -n community-profiles python src/data_management/get_map_files.py",
    "get-cer-data": "conda run -n community-profiles python src/data_management/get_cer_files.py",
    "get-tm-data": "conda run -n community-profiles python src/data_management/get_cer_files.py",
    "gis": "conda run -n community-profiles python src/data_management/cer_gis.py",
    "community-update": "npm run images && conda run -n community-profiles python src/data_management/process_communities.py",
    "traditional-territory-update": "conda run -n community-profiles python src/data_management/digital_territory.py",
    "prettier-check": "npx prettier --check .",
    "prettier": "npx prettier --write .",
    "lint": "npx eslint *.js"
  },
  "author": "Grant Moss",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "2.5.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "compression": "^1.7.4",
    "core-js": "^3.21.0",
    "express": "^4.17.2",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "leaflet": "^1.7.1",
    "mapshaper": "^0.5.91",
    "pdf2pic": "^2.1.4",
    "point-in-polygon": "^1.1.0"
  }
}
