{
  "routes": [
    {
      "route": "/GCWeb/*.css",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "route": "/GCWeb/*.js",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "route": "/shared/*.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/js/*.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}
